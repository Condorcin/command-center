{
  "name": "orbix",
  "version": "0.1.0",
  "description": "SaaS platform bootstrap with authentication - Cloudflare Workers",
  "main": "src/index.ts",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
