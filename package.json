{
  "name": "orbix",
  "version": "0.1.0",
  "description": "SaaS platform bootstrap with authentication - Cloudflare Workers",
  "main": "src/index.ts",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "db:migrate": "wrangler d1 migrations apply orbix-db",
    "db:migrate:local": "wrangler d1 migrations apply orbix-db --local",
    "db:create": "wrangler d1 create orbix-db",
    "db:export": "bash scripts/export-db.sh",
    "db:export:prod": "bash scripts/export-db-prod.sh",
    "db:migrate:items": "bash scripts/migrate-items-to-prod.sh",
    "db:migrate:incremental": "bash scripts/migrate-incremental.sh",
    "db:migrate:data": "bash scripts/migrate-items-simple.sh",
    "db:migrate:data:incremental": "node scripts/migrate-data-to-prod.js --incremental",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "cloudflare-workers",
    "saas",
    "authentication"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241127.0",
    "@types/node": "^20.11.0",
    "typescript": "^5.3.3",
    "wrangler": "^3.22.0"
  },
  "dependencies": {
    "@noble/hashes": "^1.3.3"
  }
}




