  "keywords": [
    "cloudflare-workers",
    "saas",
    "authentication"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241127.0",
    "@types/node": "^20.11.0",
    "typescript": "^5.3.3",
    "wrangler": "^3.22.0"
  },
  "dependencies": {
    "@noble/hashes": "^1.3.3"
  }
}




